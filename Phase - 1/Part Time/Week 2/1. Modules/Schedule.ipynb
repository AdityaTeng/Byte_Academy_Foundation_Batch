    "import winsound\n",
    "import schedule\n",
    "import time\n",
    "\n",
    "\n",
    "def Beep():\n",
    "    f = 2000\n",
    "    d = 1000\n",
    "    winsound.Beep(f, d)\n",
    "\n",
    "schedule.every(15).minutes.do(Beep)\n",
    "\n",
    "while 1:\n",
    "    schedule.run_pending()\n",
    "    time.sleep(1)"
   ]
